<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hollow.thesolitaryowl.negotium">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/stetlogo"
        android:label="MedDiary"
        android:roundIcon="@drawable/medpill"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddTaskActivity"/>
        <activity android:name=".ViewTaskActivity" />
        <activity android:name=".TaskDetailActivity"></activity>
    </application>

</manifest>
